{
    "github": {
        "silent": true
    },
    "buildCommand": "flutter build web --release --web-renderer canvaskit",
    "outputDirectory": "build/web",
    "installCommand": "flutter pub get",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}
